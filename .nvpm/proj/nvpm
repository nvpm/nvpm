

# seng  {

*workspace seng

  tab main
    buff case: seng/case.sng
    buff bugs: seng/bugs.sng
    buff idea: seng/idea.sng
    buff save: seng/save.sng

# }
# Plugin  {

workspace NVPM-Plugin

  tab Main
    buff plugin   : plugin/nvpm.vim
    buff init.vim : ~/.config/nvim/init.vim
   *buff readme   : README.md

 *tab Colors
    buff syntax : syntax/nvpm.vim
    buff proj   : .nvpm/proj/plugin

  tab Doc
    buff doc : doc/nvpm.txt
    buff read: README.md
    buff lice: LICENSE
    buff vers: version

# }
# VimDocs {

workspace Vim-Docs

  tab Syntax
    buff syntax  : /i/apps/share/nvim/runtime/doc/syntax.txt
    buff usr_06  : /i/apps/share/nvim/runtime/doc/usr_06.txt
    buff usr_44  : /i/apps/share/nvim/runtime/doc/usr_44.txt

  tab VimLang
    buff visual  : /i/apps/share/nvim/runtime/doc/visual.txt
    buff eval    : /i/apps/share/nvim/runtime/doc/eval.txt
    buff map     : /i/apps/share/nvim/runtime/doc/map.txt
    buff windows : /i/apps/share/nvim/runtime/doc/windows.txt
    buff options : /i/apps/share/nvim/runtime/doc/options.txt

  *tab Main
    term Ranger : ranger /usr/share/nvim/runtime/doc

#}
